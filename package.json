{
    "name": "ets-log210-typescript-node-api",
    "version": "2.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "tsc -b --verbose && shx cp -ru public dist/",
        "start": "node dist/index.js",
        "watch": "jest --watch --coverage test",
        "test": "jest --env=node --colors --coverage test",
        "uml-classes-puml": "tplant -i src/App.ts -A -o App.puml",
        "uml-classes-svg": "tplant -i src/App.ts -A -o App.svg",
        "circular": "madge src/App.ts -c",
        "madge": "madge src/App.ts -i madge.png; open madge.png"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "npm run build && npm start",
        "ext": "ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/debug": "^4.1.6",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/jest": "^26.0.24",
        "@types/morgan": "^1.9.3",
        "@types/node": "^16.3.0",
        "@types/supertest": "^2.0.11",
        "coveralls": "^3.1.1",
        "gitinspector": "^0.5.0-dev-2",
        "jest": "^27.0.6",
        "jest-extended": "^0.11.5",
        "madge": "^5.0.1",
        "nodemon": "^2.0.12",
        "shx": "^0.3.3",
        "supertest": "^6.1.3",
        "ts-jest": "^27.0.3",
        "ts-node": "^10.0.0",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "debug": "^4.3.2",
        "express": "^4.17.1",
        "express-flash-plus": "^0.0.7",
        "express-session": "^1.17.2",
        "morgan": "^1.10.0",
        "pug": "^3.0.2",
        "tplant": "^3.0.0"
    }
}
